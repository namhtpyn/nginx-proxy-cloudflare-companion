{
  "name": "nginx-proxy-cloudflare-companion",
  "version": "1.0.0",
  "description": "A simple node.js proxy server that forwards requests to a specified URL",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:docker": "docker build .",
    "start:dev": "tsx --env-file=.env.defaults --env-file=.env src/index.ts",
    "start:prod": "node dist/index.js"
  },
  "dependencies": {
    "zod": "3.23.8",
    "dockerode": "4.0.2",
    "cloudflare": "3.2.0",
    "tldts": "6.1.20",
    "public-ip": "6.0.2",
    "cron": "3.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "typescript": "5.4.5",
    "tsx": "4.10.2",
    "@types/dockerode": "3.3.29"
  }
}
